<div class="banner" >

  <%# <%= image_tag 'cinema.jpg'%>

  <div class="container">
    <h1> Movies </h1>
  </div>
</div>

<div class="title">
  <h2> My lists </h2>
</div>

<div class="cards">
  <% @lists.each do |list| %>
  <div class="card-category">
    <h3 class="list-name">
      <%= link_to list.name, list_path(list), class: 'list-item' %>
    </h3>
  </div>
  <% end %>
</div>
